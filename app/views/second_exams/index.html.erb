<p id="notice"><%= notice %></p>

<h1>Second Exams</h1>

<table>
  <thead>
    <tr>
      <th>Second exam</th>
      <th>Grade</th>
      <th>Course</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @second_exams.each do |second_exam| %>
      <tr>
        <td><%= second_exam.second_exam_id %></td>
        <td><%= second_exam.grade %></td>
        <td><%= second_exam.course_id %></td>
        <td><%= second_exam.student_id %></td>
        <td><%= link_to 'Show', second_exam %></td>
        <td><%= link_to 'Edit', edit_second_exam_path(second_exam) %></td>
        <td><%= link_to 'Destroy', second_exam, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Second Exam', new_second_exam_path %>
